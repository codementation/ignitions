{
  "ignition": {
    "version": "3.1.0"
  },
  "passwd": {
    "users": [
      {
        "name": "coreos",
        "sshAuthorizedKeys": [
          "ssh-rsa  battlestation"
        ]
      }
    ]
  },
  "storage": {
    "files": [
      {
        "overwrite": true,
        "path": "/etc/NetworkManager/system-connections/ens3.nmconnection",
        "contents": {
          "source": "data:,%5Bconnection%5D%0Atype%3Dethernet%0Aid%3D'Custom%20interface'%0Ainterface-name%3Dens3%0A%0A%5Bipv4%5D%0Amethod%3Dmanual%0Aaddresses%3D192.168.88.101%2F24%0Agateway%3D192.168.88.1%0Adns%3D1.1.1.1%3B8.8.8.8%0A"
        },
        "mode": 384
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "contents": "[Unit]\nDescription=Run CoreOS Installer\nRequires=coreos-installer-pre.target\nAfter=coreos-installer-pre.target\nAfter=network-online.target\nWants=network-online.target\nOnFailure=emergency.target\nOnFailureJobMode=replace-irreversibly\n\n[Service]\nType=oneshot\nExecStart=/usr/bin/coreos-installer install -n --ignition-url https://raw.githubusercontent.com/codementation/ignitions/master/installer-ign-slave00.json /dev/vda \nExecStart=/usr/bin/systemctl --no-block poweroff\nStandardOutput=kmsg+console\nStandardError=kmsg+console\n\n[Install]\nRequiredBy=default.target\n",
        "enabled": true,
        "name": "coreos-installer.service"
      }
    ]
  }
}
